{
  "name": "beecrowd-solutions-in-ts",
  "version": "0.0.1",
  "description": "Question codes approved in URI Online Judge (or Beecrowd) site using only modern TS syntax (5.1+)",
  "private": "true",
  "type": "module",
  "scripts": {
    "test": "tsx --test ./test/**/*.test.ts",
    "lint": "biome lint ./src --diagnostic-level=error",
    "format": "biome format ./src --write --diagnostic-level=warn",
    "prepare": "husky install"
  },
  "author": {
    "name": "David Wesley",
    "email": "contato.dwesley@outlook.com",
    "url": "https://github.com/DavidWesley"
  },
  "keywords": [
    "typescript",
    "uri-online-judge",
    "beecrowd",
    "beecrowd-solutions",
    "beecrowd-solutions-in-ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidWesley/beecrowd-solutions-in-ts.git"
  },
  "bugs": {
    "url": "https://github.com/DavidWesley/beecrowd-solutions-in-ts/issues"
  },
  "homepage": "https://github.com/DavidWesley/beecrowd-solutions-in-ts#readme",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^20.4.10",
    "cross-env": "^7.0.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "tsx": "^4.7.0",
    "typescript": "5.1.6"
  }
}
